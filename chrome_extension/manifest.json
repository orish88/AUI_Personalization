{
  "manifest_version": 2,

  "name": "AUI popup1",
  "description": "This extension allows the user to personalize the current page  .",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png"
  },
  "permissions": [
    "activeTab",
    "storage"
  ],
  "background": {
    "scripts": ["thirdParty/jquery.js", "background.js"]
  }

  
}
