{
  "manifest_version": 4,

  "name": "AUI Personalization",
  "description": "This extension allows the user to personalize the current page  .",
  "version": "1.0",

  "browser_action": {
    "default_icon": "athena_logo.png",
    "default_popup": "popup_e.html"
  },
  "permissions": [
    "activeTab",
    "storage",
    "tabs",
    "*://*/*"
  ]

}
